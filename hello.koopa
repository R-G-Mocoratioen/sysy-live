fun @main(): i32 {
%0:
  %1 = alloc i32
  store 1, %1
  %2 = load %1
  %3 = eq %2, 1
  %4 = alloc i32
  store 0, %4
  br %3, %5, %6

%5:
  %7 = load %1
  %8 = eq %7, 2
  %9 = ne %8, 0
  store %9, %4
  jump %6

%6:
  %10 = load %4
  br %10, %11, %12

%11:
  store 5, %1
  jump %12

%12:
  %13 = load %1
  ret %13

%14:
  ret 0
}
