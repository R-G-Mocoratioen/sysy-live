fun @f(@arr: *[[i32, 2], 2]): i32 {
%entry:
  %arr = alloc *[[i32, 2], 2]
  store @arr, %arr
  %0 = load %arr
  %1 = getptr %0, 1
  %3 = getelemptr %1, 0
  %4 = getelemptr %3, 0
  %5 = load %4
  ret %5
}

fun @main(): i32 {
%entry:
  @arr = alloc [[[i32, 2], 2], 2]
  %2 = getelemptr @arr, 1
  %3 = call @f(%2)
  ret 0
}